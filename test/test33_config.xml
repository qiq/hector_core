<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "hector_config.dtd">
<!--
test33: read-write: one reader(1), one writer(1), using TestPerlResource
-->
<Config>
	<Server id="test" lib="TestServer.la">
		<threads>2</threads>
		<serverHost>*</serverHost>
		<serverPort>1101</serverPort>
		<logConfig>test.log.props</logConfig>
		<resources>
			<Resource id="R_test" type="perl" lib="TestPerlResource.pm"/>
		</resources>
		<ProcessingEngine id="PE_test">
			<Processor id="P_input">
				<threads>1</threads>
				<modules>
					<Module id="M_input" lib="TestInput.la">
						<param name="maxItems" value="1000"/>
						<param name="resourceType" value="TestPerlResource"/>
					</Module>
				</modules>
				<output>
					<nextProcessor ref="P_output"/>
				</output>
			</Processor>
			<Processor id="P_output">
				<threads>1</threads>
				<modules>
					<Module id="M_output" lib="TestOutput.la"/>
				</modules>
				<input>
					<queue/>
				</input>
			</Processor>
		</ProcessingEngine>
	</Server>
</Config>
