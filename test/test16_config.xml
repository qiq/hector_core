<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "hector_config.dtd">
<!--
test6: sort according to status: two readers(1), connected to two intermediates(1), another simple intermediate, two writers(1)
-->
<Config>
	<Server id="test" lib="TestServer.la">
		<threads>1</threads>
		<serverHost>*</serverHost>
		<serverPort>1101</serverPort>
		<logConfig>test.log.props</logConfig>
		<resources>
			<Resource id="R_test" lib="TestResource.la"/>
		</resources>
		<ProcessingEngine id="PE_test">
			<Processor id="P_input1">
				<threads>1</threads>
				<modules>
					<Module id="M_input1" type="perl" lib="TestInput.pm">
						<param name="maxItems" value="1000"/>
						<param name="idPrefix" value="i1-"/>
					</Module>
					<Module id="M_simple1" type="perl" lib="TestSimple.pm">
						<param name="setStatus" value="1"/>
					</Module>
				</modules>
				<output>
					<nextProcessor ref="P_simple"/>
				</output>
			</Processor>
			<Processor id="P_input2">
				<threads>1</threads>
				<modules>
					<Module id="M_input2" type="perl" lib="TestInput.pm">
						<param name="maxItems" value="2000"/>
						<param name="idPrefix" value="i2-"/>
					</Module>
					<Module id="M_simple2" type="perl" lib="TestSimple.pm">
						<param name="setStatus" value="0"/>
					</Module>
				</modules>
				<output>
					<nextProcessor ref="P_simple"/>
				</output>
			</Processor>
			<Processor id="P_simple">
				<threads>1</threads>
				<modules>
					<Module id="M_simple" type="perl" lib="TestSimple.pm"/>
				</modules>
				<input>
					<queue/>
				</input>
				<output>
					<nextProcessor ref="P_output1" filter="1"/>
					<nextProcessor ref="P_output2"/>
				</output>
			</Processor>
			<Processor id="P_output1">
				<threads>1</threads>
				<modules>
					<Module id="M_output1" type="perl" lib="TestOutput.pm"/>
				</modules>
				<input>
					<queue/>
				</input>
			</Processor>
			<Processor id="P_output2">
				<threads>1</threads>
				<modules>
					<Module id="M_output2" type="perl" lib="TestOutput.pm"/>
				</modules>
				<input>
					<queue/>
				</input>
			</Processor>
		</ProcessingEngine>
	</Server>
</Config>
