<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "hector_config.dtd">
<!--
test21: accessing processing chain from the server
-->
<Config>
	<Server id="test" lib="TestServer.la">
		<threads>1</threads>
		<serverHost>*</serverHost>
		<serverPort>1101</serverPort>
		<logConfig>test.log.props</logConfig>
		<resources>
			<Resource id="R_test" lib="TestResource.la"/>
		</resources>
		<ProcessingEngine id="PE_test">
			<inputProcessor ref="P_simple"/>
			<Processor id="P_simple">
				<threads>1</threads>
				<modules>
					<Module id="M_simple" lib="TestSimple.la"/>
				</modules>
				<input>
					<queue/>
				</input>
				<output>
					<nextProcessor ref="PE_test"/>
				</output>
			</Processor>
		</ProcessingEngine>
	</Server>
</Config>
