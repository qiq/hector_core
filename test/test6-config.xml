<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "helen_config.dtd">
<!--
test6: sort according to status: two readers(1), connected to two intermediates(1), another simple intermediate, two writers(1)
-->
<Config>
	<Server id="test" lib="../lib/processing_engine/servers/TestServer.la">
		<threads>1</threads>
		<serverHost>*</serverHost>
		<serverPort>1100</serverPort>
		<logConfig>test.log.props</logConfig>
		<ProcessingEngine id="robot_processing_engine">
			<Processor id="P_load_resource1">
				<threads>1</threads>
				<modules>
					<Module id="M_load_resource1" lib="../lib/processing_engine/modules/TestInput.la">
						<param name="maxItems" value="1000"/>
						<param name="idPrefix" value="i1-"/>
					</Module>
					<Module id="M_simple1" lib="../lib/processing_engine/modules/TestSimple.la">
						<param name="setStatus" value="1"/>
					</Module>
				</modules>
				<output>
					<nextProcessor ref="P_simple"/>
				</output>
			</Processor>
			<Processor id="P_load_resource2">
				<threads>1</threads>
				<modules>
					<Module id="M_load_resource2" lib="../lib/processing_engine/modules/TestInput.la">
						<param name="maxItems" value="2000"/>
						<param name="idPrefix" value="i2-"/>
					</Module>
					<Module id="M_simple2" lib="../lib/processing_engine/modules/TestSimple.la">
						<param name="setStatus" value="0"/>
					</Module>
				</modules>
				<output>
					<nextProcessor ref="P_simple"/>
				</output>
			</Processor>
			<Processor id="P_simple">
				<threads>1</threads>
				<modules>
					<Module id="M_simple" lib="../lib/processing_engine/modules/TestSimple.la"/>
				</modules>
				<input>
					<queue/>
				</input>
				<output>
					<nextProcessor ref="P_save_resource1" filter="1"/>
					<nextProcessor ref="P_save_resource2"/>
				</output>
			</Processor>
			<Processor id="P_save_resource1">
				<threads>1</threads>
				<modules>
					<Module id="M_save_resource1" lib="../lib/processing_engine/modules/TestOutput.la"/>
				</modules>
				<input>
					<queue/>
				</input>
			</Processor>
			<Processor id="P_save_resource2">
				<threads>1</threads>
				<modules>
					<Module id="M_save_resource2" lib="../lib/processing_engine/modules/TestOutput.la"/>
				</modules>
				<input>
					<queue/>
				</input>
			</Processor>
		</ProcessingEngine>
	</Server>
</Config>
